  Citeulike-linkout-1      = {http://link.aip.org/link/?JCP/125/054108},
  Keywords                 = {committor},
  Posted-at                = {2008-11-23 16:58:13},
  Priority                 = {2},
  Publisher                = {AIP},
  Url                      = {http://scitation.aip.org/getabs/servlet/GetabsServlet?prog=normal\&id=JCPSA6000125000005054108000001\&idtype=cvips\&gifs=yes}
