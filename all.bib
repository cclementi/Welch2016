@Article{MolskiHofkens_ChemPhysLett00,
  Title                    = {{Theory of time-resolved single-molecule fluorescence spectroscopy}},
  Author                   = {Molskia, Andrzej and Hofkens, Johan and Genscha, Thomas and Boensa, No\"{e}l and De Schryvera, Frans},
