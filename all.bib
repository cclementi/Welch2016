@Article{ZwanzigAilawadi_PRE69_StatisticalErrorCorrelations,
  Title                    = {{Statistical Error Due to Finite Time Averaging in Computer Experiments}},
  Author                   = {Zwanzig, Robert and Ailawadi, Narinder K.},
  Journal                  = {Physical Review Online Archive (Prola)},
  Year                     = {1969},

  Month                    = jun,
  Pages                    = {280--283},
  Volume                   = {182},
  Abstract                 = {{In using a computer experiment to calculate the time correlation function of some dynamical variable, the ensemble average over an equilibrium distribution in phase space often is replaced by a time average over a finite interval T. It is shown here that the statistical uncertainty due to this kind of average is of the order of 1/T1/2. The coefficient of the square root is related to a characteristic relaxation time of the correlation function. If only time averaging is performed, the resulting statistical uncertainty in typical computer experiments may be of the order of 20\%.}},
  Bdsk-url-1               = {http://dx.doi.org/10.1103/PhysRev.182.280},
  Citeulike-article-id     = {10374846},
  Citeulike-linkout-0      = {http://dx.doi.org/10.1103/PhysRev.182.280},
  Citeulike-linkout-1      = {http://link.aps.org/abstract/PR/v182/i1/p280},
  Citeulike-linkout-2      = {http://link.aps.org/pdf/PR/v182/i1/p280},
  Doi                      = {10.1103/PhysRev.182.280},
