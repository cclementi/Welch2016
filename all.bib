  Citeulike-linkout-1      = {http://pubs.acs.org/doi/abs/10.1021/jp109592b},
  Day                      = {9},
  Doi                      = {10.1021/jp109592b},
  Posted-at                = {2011-08-29 16:03:38},
  Priority                 = {2},
  Publisher                = {American Chemical Society},
  Url                      = {http://dx.doi.org/10.1021/jp109592b}
}

