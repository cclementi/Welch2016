@Article{KimNienhaus_PNAS02,
  Title                    = {{Mg2+-dependent conformational change of RNA studied by fluorescence correlation and FRET on immobilized single molecules}},
  Author                   = {Kim, Harold D. and Nienhaus, G. Ulrich and Ha, Taekjip and Orr, Jeffrey W. and Williamson, James R. and Chu, Steven},
  Journal                  = {Procl. Natl. Acad. Sci. USA},
  Year                     = {2002},

  Month                    = apr,
  Number                   = {7},
  Pages                    = {4284--4289},
  Volume                   = {99},

  Abstract                 = {{10.1073/pnas.032077799 Fluorescence correlation spectroscopy (FCS) of fluorescence resonant energy transfer (FRET) on immobilized individual fluorophores was used to study the Mg-facilitated conformational change of an RNA three-helix junction, a structural element that initiates the folding of the 30S ribosomal subunit. Transitions of the RNA junction between open and folded conformations resulted in fluctuations in fluorescence by FRET. Fluorescence fluctuations occurring between two FRET states on the millisecond time scale were found to be dependent on Mg and Na concentrations. Correlation functions of the fluctuations were used to determine transition rates between the two conformations as a function of Mg or Na concentration. Both the opening and folding rates were found to vary with changing salt conditions. Assuming specific binding of divalent ions to RNA, the Mg dependence of the observed rates cannot be explained by conformational change induced by Mg binding/unbinding, but is consistent with a model in which the intrinsic conformational change of the RNA junction is altered by uptake of Mg ion(s). This version of FCS/FRET on immobilized single molecules is demonstrated to be a powerful technique in the study of conformational dynamics of biomolecules over time scales ranging from microseconds to seconds.}},
  Bdsk-url-1               = {http://dx.doi.org/10.1073/pnas.032077799},
  Citeulike-article-id     = {4132947},
  Citeulike-linkout-0      = {http://dx.doi.org/10.1073/pnas.032077799},
